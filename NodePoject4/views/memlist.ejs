<body>
~~멤바리스트~~
<table border='1'>
	<tr>
		<th>번  호</th>
		<th>이  름</th>
		<th>id</th>
		<th>pw</th>
		<th>이메일</th>
		<th>조회수</th>
	</tr>
<%

	

%>
<%
for(var i = 0; i < user.length; i++){

	var date = user[i].idate;
	
	var yr = date.getFullYear();
	var mt = date.getMonth();
	var day = date.getDay();
	var hr = date.getHours();
	var min = date.getMinutes();
	
	date = yr + '-' + (mt + 1) + '-' + day + ' ' + hr + ':' + min;
	
	console.log('date : ' + date);
	
	date = user[i].idate;
	date = moment(date).format('YYYY-MM-DD HH:mm:ss');
	console.log('date : ' + date);
	
	%>
	<tr>
		<td> <%= user[i].num %> </td>
		<td> <%= user[i].name %> </td>
		<td> <%= user[i].id %> </td>
		<td> <%= user[i].pw %> </td>
		<td> <%= user[i].email %> </td>
		<td> <%= date %> </td>
	</tr>
<%
}
%>

</table>
</body>
</html>